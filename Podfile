# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'Weather' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Weather
  pod 'SnapKit', '~> 5.0.0'

  target 'WeatherTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'SASPodTest'
  end

  target 'WeatherUITests' do
    # Pods for testing
    pod 'SASPodTest'
  end

end
